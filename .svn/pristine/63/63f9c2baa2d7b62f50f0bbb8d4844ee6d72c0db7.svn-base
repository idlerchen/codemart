package com.fenbi.codemart.bean;

/**
 * 角色类型信息的实体类，封装role_type表的信息
 *
 */
public class RoleType {

	private int id;
	private int parentId;
	private String name;

	public RoleType() {
		super();
	}

	public RoleType(int id, int parentId, String name) {
		super();
		this.id = id;
		this.parentId = parentId;
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getParentId() {
		return parentId;
	}

	public void setParentId(int parentId) {
		this.parentId = parentId;
	}

	@Override
	public String toString() {
		return "RoleType [id=" + id + ", parentId=" + parentId + ", name=" + name + "]";
	}

}
